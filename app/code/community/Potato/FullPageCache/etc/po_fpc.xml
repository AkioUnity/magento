        </welcome_message>
        <account_links>
            <name_in_layout>account.links</name_in_layout>
        </account_links>
        <wishlist/>
        <product_type_availability/>
        <product_type_data/>
        <orders/>
        <wishlist_links>
            <name_in_layout>wishlist.links</name_in_layout>
        </wishlist_links>
        <poll_activepoll>
            <name_in_layout>poll.activepoll</name_in_layout>
        </poll_activepoll>
        <widget_enterprise_banner>
            <name_in_layout>widget.enterprise.banner</name_in_layout>
        </widget_enterprise_banner>
        <catalog_product_item_related>
            <name_in_layout>catalog.product.item.related</name_in_layout>
        </catalog_product_item_related>
        <catalog_product_item_upsell>
