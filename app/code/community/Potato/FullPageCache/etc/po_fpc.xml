            <name_in_layout>catalog.compare.sidebar</name_in_layout>
        </catalog_compare_sidebar>
        <minicart_head />
        <right_poll>
            <name_in_layout>right.poll</name_in_layout>
        </right_poll>
        <right_reports_product_viewed>
            <name_in_layout>right.reports.product.viewed</name_in_layout>
        </right_reports_product_viewed>
        <right_reports_product_compared>
            <name_in_layout>right.reports.product.compared</name_in_layout>
        </right_reports_product_compared>
        <cart_top>
            <name_in_layout>cart_top</name_in_layout>
        </cart_top>
        <po_fpc_debug>
            <processor>Potato_FullPageCache_Model_Processor_Block_Session_Debug</processor>
        </po_fpc_debug>
        <formkey />
    </session_blocks>
    <allowed_routers>
        <catalog>
            <controllers>
                <category>view</category>
                <product>view</product>
            </controllers>
            <parameters>
                <processor>po_fpc/processor_router_catalog</processor>
                <lifetime>86400</lifetime>
                <skip_blocks>
                    <product_info_availability>
                        <name_in_layout>product.info.availability</name_in_layout>
                    </product_info_availability>
                    <product_info_addtocart>
                        <name_in_layout>product.info.addtocart</name_in_layout>
                    </product_info_addtocart>
                    <product_info_addto>
                        <name_in_layout>product.info.addto</name_in_layout>
                    </product_info_addto>
                    <product_info_configurable>
                        <name_in_layout>product.info.configurable</name_in_layout>
                    </product_info_configurable>
                    <product_info_options_wrapper_bottom>
                        <name_in_layout>product.info.options.wrapper.bottom</name_in_layout>
                    </product_info_options_wrapper_bottom>
                    <product_info_configurable_extra>
                        <name_in_layout>product.info.configurable.extra</name_in_layout>
                    </product_info_configurable_extra>
                    <catalog_product_list_related>
                        <name_in_layout>catalog.product.list.related</name_in_layout>
                    </catalog_product_list_related>
                    <catalog_product_list_upsell>
                        <name_in_layout>catalog.product.list.upsell</name_in_layout>
                    </catalog_product_list_upsell>
                </skip_blocks>
            </parameters>
        </catalog>
        <cms>
            <controllers>
                <page>*</page>
                <index>*</index>
            </controllers>
            <parameters>
                <lifetime>86400</lifetime>
            </parameters>
        </cms>
