            <processor>Potato_FullPageCache_Model_Processor_Block_Session_Messages</processor>
        </messages>
        <global_notices/>
        <last_viewed_products>
            <name_in_layout>last.viewed.products</name_in_layout>
        </last_viewed_products>
        <last_viewed_products_widget>
